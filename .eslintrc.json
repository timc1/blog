{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "useJSXTextNode": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"]
}
